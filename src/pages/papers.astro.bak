    }

    .venue-card {
        background: var(--background-light);
        padding: 1.5rem;
        border-radius: 8px;
        box-shadow: var(--shadow);
        text-align: center;
    }

    .venue-name {
        color: var(--primary-color);
        margin-bottom: 1rem;
        font-size: 1.1rem;
    }

