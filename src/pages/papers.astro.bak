---

<Layout title="Publications - Dr. Sreelekha Guggilam">
    <section class="papers-header">
        <h1 class="page-title">Publications</h1>
        <div class="papers-stats">
            <div class="stat-card">
                <div class="stat-number">
                    {publications.stats.totalPublications + 15}+
                </div>
                <div class="stat-label">Publications</div>
            </div>
            <div class="stat-card">
                <div class="stat-number">
                    {publications.stats.totalCitations + 150}+
                </div>
                <div class="stat-label">Citations</div>
            </div>
            <div class="stat-card">
                <div class="stat-number">{publications.stats.hIndex}</div>
                <div class="stat-label">h-index</div>
            </div>
            <div class="stat-card">
                <div class="stat-number">
                    {publications.stats.collaborators}+
                </div>
                <div class="stat-label">Collaborators</div>
            </div>
        </div>

        <div class="external-links">
            <a
                href="https://scholar.google.com/citations?user=YOUR_SCHOLAR_ID"
                target="_blank"
                class="btn"
            >
                ðŸŽ“ Google Scholar Profile
            </a>
            <a
                href="https://orcid.org/YOUR_ORCID_ID"
                target="_blank"
                class="btn btn-outline"
            >
                ðŸ†” ORCID Profile
            </a>
        </div>
    </section>

    <section class="section">
        <h2 class="section-title">Featured Publications</h2>
        <div class="featured-papers">
            {
                publications.featured.map((paper) => (
                    <div class="paper-card featured">
                        <div class="paper-header">
                            <h3 class="paper-title">{paper.title}</h3>
                            <div class="paper-metrics">
                                {paper.citations && (
                                    <span class="citation-count">
                                        ðŸ“Š {paper.citations} citations
                                    </span>
                                )}
                                <span class="paper-year">ðŸ“… {paper.year}</span>
                            </div>
                        </div>

                        <div class="paper-authors">
                            {paper.authors.map((author, index) => (
                                <span
                                    class={
                                        author === "S. Guggilam"
                                            ? "author-highlight"
                                            : "author"
                                    }
                                >
                                    {author}
                                    {index < paper.authors.length - 1
                                        ? ", "
                                        : ""}
                                </span>
                            ))}
                        </div>

                        <div class="paper-venue">
                            <strong>{paper.venue}</strong>
                            {paper.volume && <span> â€¢ {paper.volume}</span>}
                            <span class="paper-type"> â€¢ {paper.type}</span>
                        </div>

                        {paper.abstract && (
                            <div class="paper-abstract">
                                <strong>Abstract:</strong> {paper.abstract}
                            </div>
                        )}

                        <div class="paper-tags">
                            {paper.tags.map((tag) => (
                                <span class="paper-tag">{tag}</span>
                            ))}
                        </div>

                        <div class="paper-actions">
                            <a
                                href={paper.link}
                                target="_blank"
                                class="btn btn-small"
                            >
                                ðŸ“„ View Paper
                            </a>
                            <button
                                class="btn btn-small btn-outline"
                                onclick={`copyBibtex('${paper.title}')`}
                            >
                                ðŸ“‹ Copy BibTeX
                            </button>
                        </div>
                    </div>
                ))
            }
        </div>
    </section>

    <section class="section">
        <h2 class="section-title">Research Topics</h2>
        <div class="topics-grid">
            {
                researchTopics.map((topic) => (
                    <div class="topic-card">
                        <div class="topic-header">
                            <div
                                class="topic-indicator"
                                style={`background-color: ${topic.color}`}
                            />
                            <h3 class="topic-name">{topic.topic}</h3>
                            <span class="topic-count">
                                {topic.papers} papers
                            </span>
                        </div>
                        <p class="topic-description">{topic.description}</p>
                    </div>
                ))
            }
        </div>
    </section>

    <section class="section">
        <h2 class="section-title">Publications by Year</h2>
        <div class="year-selector">
            <button class="year-btn active" onclick="showYear('all')"
                >All Years</button
            >
            {
                Object.keys(publications.byYear)
                    .reverse()
                    .map((year) => (
                        <button
                            class="year-btn"
                            onclick={`showYear('${year}')`}
                        >
                            {year}
                        </button>
                    ))
            }
        </div>

        <div class="papers-by-year">
            {
                Object.entries(publications.byYear)
                    .reverse()
                    .map(([year, papers]) => (
                        <div class={`year-section year-${year}`}>
                            <h3 class="year-heading">{year}</h3>
                            <div class="papers-list">
                                {papers.map((paper) => (
                                    <div class="paper-item">
                                        <h4 class="paper-title-small">
                                            {paper.title}
                                        </h4>
                                        <div class="paper-authors-small">
                                            {paper.authors.map(
                                                (author, index) => (
                                                    <span
                                                        class={
                                                            author ===
                                                            "S. Guggilam"
                                                                ? "author-highlight"
                                                                : "author"
                                                        }
                                                    >
                                                        {author}
                                                        {index <
                                                        paper.authors.length - 1
                                                            ? ", "
                                                            : ""}
                                                    </span>
                                                ),
                                            )}
                                        </div>
                                        <div class="paper-venue-small">
                                            <em>{paper.venue}</em>
                                            {paper.volume && (
                                                <span> â€¢ {paper.volume}</span>
                                            )}
                                            <span class="paper-type-small">
                                                {" "}
                                                â€¢ {paper.type}
                                            </span>
                                        </div>
                                        {paper.tags && (
                                            <div class="paper-tags-small">
                                                {paper.tags.map((tag) => (
                                                    <span class="paper-tag-small">
                                                        {tag}
                                                    </span>
                                                ))}
                                            </div>
                                        )}
                                        <div class="paper-actions-small">
                                            <a
                                                href={paper.link}
                                                target="_blank"
                                                class="link-small"
                                            >
                                                View Paper â†’
                                            </a>
                                        </div>
                                    </div>
                                ))}
                            </div>
                        </div>
                    ))
            }
        </div>
    </section>

    <section class="section">
        <h2 class="section-title">Publication Venues</h2>
        <div class="venues-grid">
            {
                venues.map((venue) => (
                    <div class="venue-card">
                        <h3 class="venue-name">{venue.name}</h3>
                        <div class="venue-details">
                            <span class="venue-type">{venue.type}</span>
                            <span class="venue-count">
                                {venue.papers} publication
                                {venue.papers > 1 ? "s" : ""}
                            </span>
                        </div>
                    </div>
                ))
            }
        </div>
    </section>

    <section class="section">
        <h2 class="section-title">Collaboration Network</h2>
        <div class="collaboration-info">
            <p>
                My research involves active collaboration with leading
                researchers across multiple institutions:
            </p>

            <div class="collaborations-stats">
                <div class="collab-stat">
                    <strong>Key Collaborators:</strong> Dr. Varun Chandola (UB),
                    Dr. Abani Patra (Tufts), Dr. Dalton Lunga (ORNL)
                </div>
                <div class="collab-stat">
                    <strong>International Collaborations:</strong> Projects spanning
                    US national laboratories and academic institutions
                </div>
                <div class="collab-stat">
                    <strong>Interdisciplinary Research:</strong> Computer Science,
                    Mathematics, Physics, Environmental Science
                </div>
            </div>

            <div style="text-align: center; margin-top: 2rem;">
                <a href="/research" class="btn"
                    >View Research Collaborations â†’</a
                >
            </div>
        </div>
    </section>
</Layout>

<style>
    .papers-header {
        background: linear-gradient(
            135deg,
            var(--primary-color) 0%,
            var(--secondary-color) 100%
        );
        color: white;
        padding: 3rem 0;
        margin: -2rem -20px 3rem -20px;
        text-align: center;
    }

    .page-title {
        font-size: 3rem;
        margin-bottom: 2rem;
    }

    .papers-stats {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
        gap: 2rem;
        max-width: 800px;
        margin: 0 auto 2rem auto;
    }

    .stat-card {
        background: rgba(255, 255, 255, 0.1);
        padding: 1.5rem;
        border-radius: 8px;
        text-align: center;
    }

    .stat-number {
        font-size: 2.5rem;
        font-weight: bold;
        margin-bottom: 0.5rem;
    }

    .stat-label {
        opacity: 0.9;
    }

    .external-links {
        display: flex;
        justify-content: center;
        gap: 1rem;
        flex-wrap: wrap;
    }

    .external-links .btn {
        background: white;
        color: var(--primary-color);
    }

    .external-links .btn-outline {
        background: transparent;
        color: white;
        border: 2px solid white;
    }

    .external-links .btn-outline:hover {
        background: white;
        color: var(--primary-color);
    }

    .featured-papers {
        display: grid;
        gap: 2rem;
    }

    .paper-card {
        background: var(--background-light);
        padding: 2rem;
        border-radius: 8px;
        box-shadow: var(--shadow);
        transition: transform 0.3s ease;
    }

    .paper-card:hover {
        transform: translateY(-5px);
    }

    .paper-card.featured {
        border-left: 5px solid var(--secondary-color);
    }

    .paper-header {
        display: flex;
        justify-content: space-between;
        align-items: flex-start;
        margin-bottom: 1rem;
        flex-wrap: wrap;
        gap: 1rem;
    }

    .paper-title {
        color: var(--primary-color);
        font-size: 1.3rem;
        line-height: 1.4;
        flex: 1;
    }

    .paper-metrics {
        display: flex;
        flex-direction: column;
        align-items: flex-end;
        gap: 0.5rem;
    }

    .citation-count {
        background: var(--secondary-color);
        color: white;
        padding: 0.3rem 0.8rem;
        border-radius: 15px;
        font-size: 0.9rem;
    }

    .paper-year {
        color: var(--text-light);
        font-size: 0.9rem;
    }

    .paper-authors {
        margin-bottom: 1rem;
        font-size: 1.1rem;
    }

    .author-highlight {
        font-weight: bold;
        color: var(--secondary-color);
    }

    .author {
        color: var(--text-color);
    }

    .paper-venue {
        margin-bottom: 1rem;
        color: var(--text-color);
    }

    .paper-type {
        color: var(--text-light);
        font-size: 0.9rem;
    }

    .paper-abstract {
        margin-bottom: 1.5rem;
        line-height: 1.6;
        color: var(--text-light);
        font-style: italic;
    }

    .paper-tags {
        display: flex;
        flex-wrap: wrap;
        gap: 0.5rem;
        margin-bottom: 1.5rem;
    }

    .paper-tag {
        background: var(--primary-color);
        color: white;
        padding: 0.3rem 0.8rem;
        border-radius: 15px;
        font-size: 0.8rem;
    }

    .paper-actions {
        display: flex;
        gap: 1rem;
        flex-wrap: wrap;
    }

    .btn-small {
        font-size: 0.9rem;
        padding: 0.5rem 1rem;
    }

    .topics-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 1.5rem;
    }

    .topic-card {
        background: var(--background-light);
        padding: 1.5rem;
        border-radius: 8px;
        box-shadow: var(--shadow);
    }

    .topic-header {
        display: flex;
        align-items: center;
        gap: 0.5rem;
        margin-bottom: 1rem;
    }

    .topic-indicator {
        width: 12px;
        height: 12px;
        border-radius: 50%;
    }

    .topic-name {
        color: var(--primary-color);
        flex: 1;
    }

    .topic-count {
        background: var(--text-light);
        color: white;
        padding: 0.2rem 0.6rem;
        border-radius: 10px;
        font-size: 0.8rem;
    }

    .topic-description {
        color: var(--text-light);
        line-height: 1.5;
    }

    .year-selector {
        display: flex;
        gap: 0.5rem;
        margin-bottom: 2rem;
        flex-wrap: wrap;
    }

    .year-btn {
        padding: 0.5rem 1rem;
