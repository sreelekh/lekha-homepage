    }

    .paper-authors-small {
        margin-bottom: 0.5rem;
    }

    .paper-venue-small {
        margin-bottom: 1rem;
        color: var(--text-color);
    }

    .paper-type-small {
        color: var(--text-light);
        font-size: 0.9rem;
    }

    .paper-tags-small {
        display: flex;
        flex-wrap: wrap;
        gap: 0.3rem;
        margin-bottom: 1rem;
    }

    .paper-tag-small {
        background: var(--text-light);
        color: white;
        padding: 0.2rem 0.6rem;
        border-radius: 10px;
        font-size: 0.7rem;
    }

    .link-small {
        color: var(--secondary-color);
        text-decoration: none;
        font-weight: 500;
    }

    .link-small:hover {
